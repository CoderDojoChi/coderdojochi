{
  "AWSEBDockerrunVersion": 2,
  "family": "build_env",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "<USER>/<REPO>:<BUILD_NUM>",
      "memory": 512
    },
    {
      "name": "nginx",
      "image": "<USER>/<REPO>-static:<BUILD_NUM>",
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "app"
      ]
    }
  ]
}
